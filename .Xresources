!! Colors:
*foreground:                 #839496
*background:                 #002B36
*fadeColor:                  #002B36
*fading:                     40
*cursorColor:                #93A1A1
*pointerColorBackground:     #586E75
*pointerColorForeground:     #93A1A1
! Black:
*color0:                     #073642
*color8:                     #002B36
! Red:
*color1:                     #DC322F
*color9:                     #CB4B16
! Green:
*color2:                     #859900
*color10:                    #586E75
! Yellow:
*color3:                     #B58900
*color11:                    #657B83
! Blue:
*color4:                     #268BD2
*color12:                    #839496
! Magenta:
*color5:                     #D33682
*color13:                    #6C71C4
! Cyan:
*color6:                     #2AA198
*color14:                    #93A1A1
! White:
*color7:                     #EEE8D5
*color15:                    #FDF6E3
!! Xft Fonts:
Xft.dpi:                     96
Xft.antialias:               true
Xft.rgba:                    rgb
Xft.hinting:                 true
Xft.hintstyle:               hintslight
Xft.embeddedbitmap:          false
Xft.lcdfilter:               lcddefault
!! RXVT:
URxvt*termName:              xterm-256color
URxvt*geometry:              80x24
URxvt*loginShell:            true
URxvt*fading:                30
URxvt*utmpInhibit:           false
URxvt*urgentOnBell:          true
URxvt*visualBell:            true
URxvt*mapAlert:              true
URxvt*mouseWheelScrollPage:  true
URxvt*background:            #000000
URxvt*foreground:            #FFFFFF
URxvt*colorUL:               #B58900
URxvt*underlineColor:        #B58900
URxvt*depth:                 32
URxvt*lineSpace:             0
URxvt*saveLines:             8192
URxvt*buffered:              true
URxvt*hold:                  false
URxvt*internalBorder:
! Cursor:
URxvt*cursorUnderline:       true
URxvt*cursorBlink:           true
URxvt*cursorColor:           #FFFFFF
! Key bindings:
URxvt*keysym.Home:           \033OH
URxvt*keysym.End:            \033OF
URxvt*keysym.KP_Home:        \033OH
URxvt*keysym.KP_End:         \033OF
URxvt*keysym.F1:             \033OP
URxvt*keysym.F2:             \033OQ
URxvt*keysym.F3:             \033OR
URxvt*keysym.F4:             \033OS
URxvt*keysym.S-Delete:       \033[3;2~
URxvt*keysym.S-Home:         \033[1;2H
URxvt*keysym.S-End:          \033[1;2F
URxvt*keysym.S-F1:           \033[1;2P
URxvt*keysym.S-F2:           \033[1;2Q
URxvt*keysym.S-F3:           \033[1;2R
URxvt*keysym.S-F4:           \033[1;2S
URxvt*keysym.S-F5:           \033[15;2~
URxvt*keysym.S-F6:           \033[17;2~
URxvt*keysym.S-F7:           \033[18;2~
URxvt*keysym.S-F8:           \033[19;2~
URxvt*keysym.S-F9:           \033[20;2~
URxvt*keysym.S-F10:          \033[21;2~
URxvt*keysym.S-F11:          \033[23;2~
URxvt*keysym.S-F12:          \033[24;2~
URxvt*keysym.Shift-Up:       command:\033]720;1\007
URxvt*keysym.Shift-Down:     command:\033]721;1\007

URxvt*iso14755:              false
URxvt*iso14755_52:           false
! Set font:
URxvt*font:                  xft:           DejaVu Sans Mono:  size=11
! Reduce spacing:
URxvt*letterSpace:           -1
! Disable perl support:
URxvt*perl-ext:
URxvt*perl-ext-common:
! Scrollbar:
URxvt*scrollColor:           #777777
URxvt*scrollstyle:           plain
URxvt*scrollTtyKeypress:     true
URxvt*scrollTtyOutput:       false
URxvt*scrollWithBuffer:      false
URxvt*scrollBar_right:       true
URxvt*skipScroll:            false
URxvt*scrollBar:             false
URxvt*scrollBar_floating:    false
